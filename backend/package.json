{
  "name": "type-pen",
  "version": "3.0.0",
  "description": "A TypeScript-based backend starter with PostgreSQL, Express, and Node.js. Flexible and frontend-agnosticâ€”connect with React, Angular, Vue, or any framework!",
  "main": "index.js",
  "scripts": {
    "start": "tsc -b && node ./dist/index.js",
    "dev": "npx prisma generate && npm run start",
    "migrate": "npx prisma migrate dev",
    "db:clear": "npx prisma migrate reset",
    "generate": "npx prisma generate",
    "build": "npm i && tsc -b",
    "setup": "npm i",
    "db": "docker rm -f typepen-postgres || true && docker-compose up -d",
    "db:clean": "docker rm -f typepen-postgres && docker-compose up -d",
    "db:reset": "docker-compose down && docker-compose up -d"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MeerUzairWasHere/TypePEN.git"
  },
  "keywords": [
